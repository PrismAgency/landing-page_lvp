ErrorDocument 404 /

SetEnv PHP_VER 5_4
SetEnv REGISTER_GLOBALS 0

RewriteEngine On

# FOR HTTPS

#RewriteCond %{SERVER_PORT} 80
#RewriteRule ^(.*)$ https://devel.lavaguepourpre.com/$1 [R,L]

RewriteRule home/ /index.php [L]
RewriteRule succes([a-zA-Z0-9\-]+)-([a-zA-Z0-9\-]+)/([a-zA-Z0-9\-]+)/([a-zA-Z0-9\-]+)/ index.php?send=ok [L]
RewriteRule error([a-zA-Z0-9\-]+)-([a-zA-Z0-9\-]+)/([a-zA-Z0-9\-]+)/([a-zA-Z0-9\-]+)/ index.php?send=no [L]

RewriteCond %{HTTP_HOST} ^infs.fr$
RewriteRule ^(.*) http://devel.lavaguepourpre.com/$1 [QSA,L,R=301]
